{
  "name": "pully-server",
  "version": "0.0.1",
  "description": "Youtube channel and playlist auto-downloader!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pully-svc": "./dist/bin/pully-server.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/pully-server.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/pully-server/issues"
  },
  "homepage": "https://github.com/JimmyBoh/pully-server#readme",
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/node": "10.12.4",
    "@types/yargs": "^12.0.1",
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "flexel": "^0.7.2",
    "multimatch": "^2.1.0",
    "parse-duration": "^0.1.1",
    "pully": "^2.4.1",
    "pully-core": "^0.6.0",
    "scany": "^0.9.1",
    "skedgy": "^0.7.2",
    "yargs": "^12.0.2"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js"
    ]
  }
}
